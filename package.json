    "@supabase/supabase-js": "^2.38.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.292.0",
    "next": "14.2.8",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "recharts": "^2.8.0",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.2",
    "@next/bundle-analyzer": "^14.0.0",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.0.1",
    "chrome-launcher": "^1.1.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.8",
    "jsdom": "^23.0.0",
    "lighthouse": "^11.4.0",
    "msw": "^2.0.0",
    "postcss": "^8",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "vitest": "^1.0.0",
    "wait-on": "^7.2.0"
