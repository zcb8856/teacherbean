{
  "name": "weteacher",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test --project=e2e-chromium",
    "test:e2e-ui": "playwright test --ui",
    "test:mobile": "playwright test --project=mobile-chrome",
    "test:accessibility": "playwright test --project=accessibility",
    "test:setup": "playwright test --project=setup",
    "test:cleanup": "playwright test --project=cleanup",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:install": "playwright install --with-deps",
    "audit:lighthouse": "node scripts/lighthouse-audit.js",
    "audit:lighthouse:ci": "node scripts/lighthouse-ci.js",
    "perf:analyze": "cross-env ANALYZE=true next build",
    "perf:lighthouse-dev": "concurrently \"pnpm dev\" \"wait-on http://localhost:3000 && pnpm audit:lighthouse\"",
    "accessibility:check": "node scripts/accessibility-check.js",
    "deploy:checklist": "node scripts/deployment-checklist.js",
    "deploy:checklist:interactive": "node scripts/deployment-checklist.js --interactive",
    "deploy:checklist:quick": "node scripts/deployment-checklist.js --quick",
    "deploy:verify": "node scripts/verify-setup.js",
    "analytics:report": "node scripts/weekly-analytics.js",
    "analytics:report:monthly": "node scripts/weekly-analytics.js --days=30",
    "analytics:report:custom": "node scripts/weekly-analytics.js --days=14 --top=10"
  },
  "dependencies": {
